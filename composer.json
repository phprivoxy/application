{
    "name": "phprivoxy/application",
    "description": "Framework for proxy-server applications creation with PSR15 middleware as traffic handlers.",
    "type": "application",
    "homepage": "https://www.phprivoxy.ru",
    "license": "MIT",
    "keywords": ["phprivoxy", "privoxy", "framework", "proxy-server", "php-proxy-server", "middleware", "PSR-15", "PSR15", "php", "proxyserver", "php-proxyserver", "proxy", "php-proxy", "ssl-proxy-server", "http-proxy-server", "https-proxy-server", "ssl-proxy", "http-proxy", "https-proxy", "ssl", "http", "https", "high-performance", "high-performance-proxy", "high-performance-proxy-server"],
    "autoload": {
        "psr-4": {
            "PHPrivoxy\\Application\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mikhail (Balpom)"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phprivoxy/proxy": ">=0.8.8",
        "phprivoxy/relay": ">=2.1.5",
        "psr/http-server-handler": "~1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.8"
    }
}
